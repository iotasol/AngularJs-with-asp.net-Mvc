<body class="admin-page" data-ng-app="menu-app">
  
    <div class="fluid-container no-padding">
        <div class="col-md-12 col-sm-12 col-xs-12 no-padding admin-header-bar">
            <div class="col-md-12 col-sm-12 col-xs-12 admin-header-inner-bar">
                <div class="col-md-2 col-sm-3 col-xs-6 no-padding admin-header-left-info text-center">
                    <div>
                        <h4>Dashboard</h4>
                    </div>
                </div>
                <div class="col-md-2 col-sm-4 col-xs-6 no-padding pull-right text-right admin-header-right-info">

                    <div class="col-md-9 col-sm-9 col-xs-9 no-padding custom-right-info word-break-dot">
                        Welcome@*<strong> @ViewBag.LoginUserName</strong>*@
                    </div>
                     @using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "logoutForm" })) {
                    <ul class="dropdown-menu col-md-12 col-sm-12 col-xs-12 text-left " data-role="menu" data-aria-labelledby="dropdownMenu1">
                        <li data-role="presentation"><a data-role="menuitem" tabindex="-1" href="/">Home Page</a></li>
                        @Html.AntiForgeryToken()
                        <li data-role="presentation" style="cursor: pointer;"><a href="javascript:document.getElementById('logoutForm').submit()" data-role="menuitem" tabindex="-1">Logout</a></li>
                    </ul>
                     }
                </div>
            </div>
        </div>
        <div class="clearfix"></div>
        <div class="col-md-12 no-padding admin-nav-bar-outer-containor">
            @*  @RenderSection("featured", required: false)*@
            @RenderBody()
        </div>
    </div>
   @* <div id="domMessage" style="display:none;"> 
        <h4>We are processing your request.  Please wait... </h4> <img src="../Images/ajax-loading2.gif" />
    </div>
    <span id="loginUserRole" style="display: none;">@ViewBag.LoginUserRole</span>*@
    <!-- initialize jQuery Library -->
    <script src="~/Scripts/jquery-2.1.1.min.js"></script>
    <script type="text/javascript" src="/js/bootstrap.min.js"></script>
    <!-- Include bootbox.js -->
    <script src="//oss.maxcdn.com/bootbox/4.2.0/bootbox.min.js"></script>
    <script src="~/js/jquery.blockUI.js"></script>
